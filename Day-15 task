#include <iostream>
using namespace std;

int main() {
	int T,N,i;
	cin>>T;
	while(T>0)
	{
	    cin>>N;
	    int A[N],max=0;
	    for(i=1;i<=N;i++)
	    {
	        cin>>A[i];
	        if(A[i]>max )
	        {
	            max=A[i];
	        }
	    }
	    cout<<max<<endl;
	    
	    T--;
	}
	return 0;
}
