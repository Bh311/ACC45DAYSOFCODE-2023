#include <iostream>
using namespace std;

int main() {
     int n,x,h,t,flag=0;
     cin>>n;
     cin>>h>>x;
     for(int i=0;i<n;i++)
     {
         cin>>t;
           if (t+x==h)
         {
            flag=1;
         }
     }
      if(flag==0)
      {
          cout<<"no"<<endl;
      }
      else
      {
          cout<<"yes"<<endl;
      }
         
	return 0;
}
