#include <iostream>
using namespace std;

int main() {
	int t,n;
	cin>>t;
	while(t>0)
	{   
	    cin>>n;
	    int a[n],counta=0,countb=0;
	    for(int i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }

	       if(n%2!=0)
	       {
	           cout<<"No"<<endl;
	       }
	       else
	       {
	        for(int i=0;i<n;i++)
	    {
	        if(a[i]%2==0)
	        {
	            counta++;
	        }
	        else
	        {
	            countb++;
	        }
	       }
	    if(counta==countb)
	    {
	        cout<<"Yes"<<endl;
	    }
	    else if(countb==n)
	    {
	        cout<<"Yes"<<endl;
	    }
	    else
	    {
	        cout<<"No"<<endl;
	    }
	       }
	    t--;
	    
	}
	return 0;
}
